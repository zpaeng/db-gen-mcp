{
  "name": "db-gen-mcp",
  "version": "1.0.0",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "copyfiles -u 3 src/generator/templates/*.hbs build/generator/templates/",
    "start": "node build/index.js",
    "dev": "npm run build && node build/index.js",
    "watch": "tsc --watch",
    "clean": "rimraf build",
    "rebuild": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"Linting not configured yet\"",
    "prepare": "npm run build"
  },
  "keywords": ["mcp", "database", "code-generator", "sql", "java"],
  "author": "",
  "license": "MIT",
  "description": "Database Generator MCP Server - A Model Context Protocol server for database operations and Java code generation",
  "devDependencies": {
    "@types/mssql": "^9.1.8",
    "@types/node": "^24.5.0",
    "@types/oracledb": "^6.9.1",
    "@types/pg": "^8.15.5",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.0",
    "handlebars": "^4.7.8",
    "mssql": "^11.0.1",
    "mysql2": "^3.14.5",
    "oracledb": "^6.9.0",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7",
    "zod": "^3.25.76"
  }
}
